<h1><%= header %></h1>
<ul>
  <% for(var i = 0; i < items.length; i += 1) { %>
  <li>
    <% if (items[i].current) { %>
    <strong>
      <%= items[i].name %>
    </strong>
    <% } else { %>
    <a href="<%= items[i].url %>">
      <%= items[i].name %>
    </a>
    <% } %>
  </li>
  <% } %>
</ul>
